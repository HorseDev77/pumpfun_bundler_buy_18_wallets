# Cluster: devnet | mainnet
CLUSTER=devnet

# RPC URLs (used based on CLUSTER)
MAINNET_RPC_URL=https://api.mainnet-beta.solana.com
DEVNET_RPC_URL=https://api.devnet.solana.com

# Optional WebSocket (leave empty to not use)
MAINNET_WEBSOCKET_URL=
DEVNET_WEBSOCKET_URL=

# Main wallet private key (base58) - funds distribution and token creation
MAIN_WALLET_PRIVATE_KEY=

# Number of bundler wallets to create
BUNDLER_WALLET_COUNT=10

# SOL per wallet: random amount between SOL_DISTRIBUTE_MIN and SOL_DISTRIBUTE_MAX (in SOL)
SOL_DISTRIBUTE_MIN=0.01
SOL_DISTRIBUTE_MAX=0.05

# Token metadata (for create)
TOKEN_NAME=MyToken
TOKEN_SYMBOL=MTK
TOKEN_URI=https://arweave.net/example

# Existing token mint for [7] Bundle buy only (leave empty if not using that flow)
# MINT_PUBLIC_KEY=YourExistingTokenMintBase58

# Initial buy (creator) and bundle buy amount in SOL
CREATOR_BUY_SOL=0.01
BUNDLE_BUY_SOL_MIN=0.005
BUNDLE_BUY_SOL_MAX=0.02

# Bundle provider on mainnet: jito | bloxroute
BUNDLE_PROVIDER=jito

# Jito (mainnet, when BUNDLE_PROVIDER=jito)
JITO_FEE_LAMPORTS=5000000
JITO_BLOCK_ENGINE_URL=https://tokyo.mainnet.block-engine.jito.wtf/api/v1/bundles
# Set to true to only simulate the bundle and not send
# JITO_SIMULATE_ONLY=false

# BloxRoute (mainnet, when BUNDLE_PROVIDER=bloxroute). See https://docs.bloxroute.com/solana/trader-api/api-endpoints/core-endpoints/submit-batch
# BLOXROUTE_API_URL=https://ny.solana.dex.blxrbdn.com
# BLOXROUTE_AUTH_HEADER=your-auth-header
# BLOXROUTE_TIP_LAMPORTS=1000000